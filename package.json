{
  "name": "vantr",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "npm run sort && npm run bootstrap",
    "start": "dumi dev",
    "build": "father-build",
    "build:docs": "dumi build",
    "lint": "npm run lint:script  && npm run lint:style && npm run lint:demo && npm run lint:md",
    "lint:script": "eslint . --ext '.js,.jsx,.ts,.tsx'",
    "lint:style": "stylelint 'packages/**/*.less' --syntax less",
    "lint:demo": "eslint packages/**/*.md",
    "lint:md": "remark . -f -q",
    "prettier": "prettier -c --write packages/**/*.{ts,tsx} && stylelint packages/**/*.less --fix",
    "pretty-quick": "pretty-quick",
    "test": "umi-test",
    "test:update": "npm run test --cache=false -u",
    "test:coverage": "npm run test coverage",
    "bootstrap": "lerna bootstrap --hoist",
    "sort": "sort-package-json"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "dumi": "^1.1.18",
    "dumi-theme-mobile": "1.1.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-markdown": "^2.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^32.0.1",
    "father-build": "^1.19.5",
    "husky": "^6.0.0",
    "lerna": "^4.0.0",
    "less": "^3.13.1",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "sort-package-json": "^1.50.0",
    "stylelint": "^13.13.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.2.4",
    "umi": "^3.4.21",
    "umi-test": "^1.9.6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
